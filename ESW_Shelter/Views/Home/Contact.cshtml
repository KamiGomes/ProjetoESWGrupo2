@{
    ViewData["Title"] = "Contact";
}

@section Scripts {
    <script type="text/javascript" src="~/AngScripts/libs/runtime.js"></script>
    <script type="text/javascript" src="~/AngScripts/libs/polyfills.js"></script>
    <script type="text/javascript" src="~/AngScripts/libs/styles.js"></script>
    <script type="text/javascript" src="~/AngScripts/libs/vendor.js"></script>
    <script type="text/javascript" src="~/AngScripts/libs/main.js"></script>
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<div ng-app="" ng-init="Support='Support@example.com';Marketing='Marketing@example.com'">
<address>
    One Microsoft Way<br />
    Redmond, WA 98052-6399<br />
    <abbr title="Phone">P:</abbr>
    425.555.0100
</address>

<address>
    <strong>Support:</strong> <a href="mailto:{{Support}}">{{Support}}</a><br />
    <strong>Marketing:</strong> <a href="mailto:{{Marketing}}">{{Marketing}}</a>
</address>
</div>